{
  "rules": {
    "indent": [
      1,
      2
    ],
    "quotes": [
      1,
      "single"
    ],
    "linebreak-style": [
      1,
      "unix"
    ],
    "semi": [
      1,
      "always"
    ],
    "no-console": 1,
    "no-unused-vars": 1
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": "eslint:recommended"
}